/**index.wxss**/
.page-head{
  display: flex;
  width: 100%;
  padding: 30rpx 15rpx;
  background: #fff;
  box-shadow: 0rpx 3rpx 5rpx #ccc;
  margin-bottom: 15rpx;
  align-items: center;
}
.head-item{
  flex-direction: row;
  align-items: baseline;
  padding-top:15rpx;
  padding-bottom: 15rpx; 
}

.identity{
  width: 110rpx;
  margin:0rpx 25rpx; 
  padding-left:40rpx;
  padding-right:30rpx;
  font-size: 32rpx;
  font-weight: 300;
  border: 1rpx solid #ccc;
  border-radius: 40rpx;
  box-shadow: 2rpx 2rpx 5rpx #ccc;
  letter-spacing: 8rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.identity image{
  margin-left: 8rpx;
  width: 20rpx;
  height: 50rpx;
}

.logo{
  letter-spacing: 6rpx;
  font-weight: 600;
}
.page-body{
  display: flex;
 
}
map{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  z-index: -1;
}


.nav{
  position: absolute;
  left: 0;
  bottom: 0;
}
.flex-wrp{
  display: flex;
  flex-direction: row;
  background: #fff;
}
.flex-item{
  width: 190rpx;
  padding: 20rpx 0;
  text-align: center;
}
